APP_NAME=ocr-app
PORT=3000
OLLAMA_HOST=http://host.docker.internal:11434
OLLAMA_MODEL=glm-ocr
PDF_DPI=200
OCR_TIMEOUT=120
MAX_FILE_SIZE=50
NUM_CTX=16384

# GLM-OCR SDK sidecar (layout-aware two-stage pipeline)
# docker compose defaults to http://glmocr-sidecar:5002
# Uncomment and leave empty to force direct Ollama mode:
# OCR_BACKEND=
ENABLE_LAYOUT=true
